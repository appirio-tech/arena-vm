<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB//EN"
"http://www.bea.com/servers/wls510/dtd/weblogic-ejb-jar.dtd">
<weblogic-ejb-jar>
   <weblogic-enterprise-bean>
      <ejb-name>EmailJob</ejb-name>
      <caching-descriptor>
         <max-beans-in-free-pool>200</max-beans-in-free-pool>
         <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </caching-descriptor>
      <jndi-name>com.topcoder.server.ejb.EmailServices.EmailJob</jndi-name>
   </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
      <ejb-name>EmailList</ejb-name>
      <caching-descriptor>
         <max-beans-in-free-pool>200</max-beans-in-free-pool>
         <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </caching-descriptor>
      <jndi-name>com.topcoder.server.ejb.EmailServices.EmailList</jndi-name>
   </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
      <ejb-name>EmailListGroup</ejb-name>
      <caching-descriptor>
         <max-beans-in-free-pool>200</max-beans-in-free-pool>
         <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </caching-descriptor>
      <jndi-name>com.topcoder.server.ejb.EmailServices.EmailListGroup</jndi-name>
   </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
      <ejb-name>EmailTemplate</ejb-name>
      <caching-descriptor>
         <max-beans-in-free-pool>200</max-beans-in-free-pool>
         <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </caching-descriptor>
      <jndi-name>com.topcoder.server.ejb.EmailServices.EmailTemplate</jndi-name>
   </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
      <ejb-name>EmailTemplateGroup</ejb-name>
      <caching-descriptor>
         <max-beans-in-free-pool>200</max-beans-in-free-pool>
         <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </caching-descriptor>
      <jndi-name>com.topcoder.server.ejb.EmailServices.EmailTemplateGroup</jndi-name>
   </weblogic-enterprise-bean>
</weblogic-ejb-jar>
