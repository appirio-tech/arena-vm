<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping default-lazy="false">

    <class name="com.topcoder.farm.controller.model.QueueConfig" table="FARM_QUEUE_CONFIG" lazy="true">
        <id name="id" column="ID" type="int">
            <generator class="native"/>
        </id>
        
        <property name="round" type="string" not-null="true">
			<column name="ROUND" length="12"/>
		</property>
        
        <property name="app" type="string" not-null="true">
			<column name="APP" length="40"/>
		</property>
		
		<property name="action" type="string" not-null="true">
			<column name="ACTION" length="40"/>
		</property>
		
		<property name="platform" type="string" not-null="true">
			<column name="PLATFORM" length="40"/>
		</property>
		
		<property name="practice" type="boolean" not-null="true">
			<column name="PRACTICE"/>
		</property>
        
		<property name="queueName" type="string" not-null="true">
			<column name="QUEUE_NAME" length="80"/>
		</property>
		
    </class>

</hibernate-mapping>